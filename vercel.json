{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/api/process-repository",
      "dest": "/api/process-repository.js"
    },
    {
      "src": "/api/logs",
      "dest": "/api/logs.js"
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}